{
  "name": "@esandstedt/stellaris-model",
  "version": "0.5.1",
  "description": "A library to generate an object model for a Stellaris savefile.",
  "main": "dist/index",
  "types": "dist/index",
  "files": [
    "dist/compile/**/*",
    "dist/model/**/*",
    "dist/index*"
  ],
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint && npm run typedoc",
    "typedoc": "typedoc --options ./src/typedocconfig.ts",
    "start": "ts-node src/run.ts",
    "test": "npx jest"
  },
  "author": "Erik Sandstedt",
  "license": "ISC",
  "homepage": "https://github.com/esandstedt/stellaris-model#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/esandstedt/stellaris-model.git"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.4",
    "prettier": "^1.19.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.22.11",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "jszip": "^3.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "arrowParens": "avoid",
    "trailingComma": "none"
  }
}
